//finding the frequency of element
#include <stdio.h>
int main() {
    int n, i, num;
    int count = 0;  // initialize count
    printf("Enter the number of elements: ");
    if (scanf("%d", &n) != 1) {
        printf("Invalid input\n");
        return 1;
    }
    int arr[n];
    printf("Enter the %d elements:\n", n);
    for (i = 0; i < n; i++) {
        if (scanf("%d", &arr[i]) != 1) {
            printf("Invalid input\n");
            return 1;
        }
    }
    printf("Enter the number to find frequency: ");
    if (scanf("%d", &num) != 1) {
        printf("Invalid input\n");
        return 1;
    }
    for (i = 0; i < n; i++) {
        if (arr[i] == num) {
            count++;
        }
    }
    printf("Frequency of %d = %d\n", num, count);
    return 0;
}
