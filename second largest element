// WAP to read a list of integers and store it in one dimesnional array and print the second largest integer in the list of integers
#include<stdio.h>
int main(){
    int n,i, secondlargest;
    printf("enter the number of elements:");
    scanf("%d\n",&n);
    int arr[n];
    for( int i=0 ;i<n;i++){
       // printf("enter the integer:%d\n");
        scanf("%d",&arr[i]);
    }
    
    int largest=arr[0];
    secondlargest=-999999;
    for( int i=1 ;i<n ;i++){
        if(arr[i]>largest){
            secondlargest=largest;
            largest=arr[i];
        }
    }
    if(arr[i]>secondlargest && arr[i]!=largest){
        secondlargest=arr[i];
    }
    printf("secondlargest=%d\n",secondlargest);
    return 0 ;
}
